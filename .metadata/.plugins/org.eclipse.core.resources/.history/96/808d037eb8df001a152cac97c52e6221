
public class Humain {
	public String getChat()
	{return nomDuChat;}
	public int getAnniversaire()
	{return anniversaire;}
	int taille, anniversaire;
	String nomDuChat, village;
	public Humain(int pTaille, String pChat, String pVillage, int pNaissance)
	{
	taille = pTaille;
	anniversaire = pNaissance;
	nomDuChat = pChat;
	village = pVillage;
	
	}

}
