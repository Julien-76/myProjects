import java.util.Scanner;

public class main {

	public static void main(String[] args) {
		String a;
Scanner caractere = new Scanner(System.in);
do
	{
	System.out.println("Quelle est ma bière préférée ??");
	a = caractere.nextLine();
	if (a == "Orval")
	System.out.println("Bien joué !");
	else
	System.out.println("Perdu !! Je te repose la question");
	}while (a != "Orval");
		
	}
}

